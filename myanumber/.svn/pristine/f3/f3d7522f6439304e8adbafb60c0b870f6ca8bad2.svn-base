<ResourceDictionary
   xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
   xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
   xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
   mc:Ignorable="d">
    <Style x:Key="ButtonStyle" TargetType="Button">
        <Setter Property="BorderThickness" Value="2"/>
        <Setter Property="FontSize" Value="30"/>
        <Setter Property="BorderBrush" Value="Yellow"/>
        <Setter Property="Background" Value="Yellow"/>
    </Style>

    <Style x:Key="BlueHeaderTextStyle" TargetType="TextBlock">
        <Setter Property="FontSize" Value="31"/>
        <Setter Property="Foreground" Value="#FF0077B7" />
        <Setter Property="FontWeight" Value="Bold" />
        <Setter Property="FontFamily" Value="Century Gothic"/>
    </Style>

    <Style x:Key="OrangeHeaderTextStyle" TargetType="TextBlock">
        <Setter Property="FontSize" Value="31"/>
        <Setter Property="Foreground" Value="Orange" />
        <Setter Property="FontWeight" Value="Bold" />
        <Setter Property="FontFamily" Value="Century Gothic"/>
    </Style>

    <Style x:Key="HeaderTextStyle" TargetType="TextBlock">
        <Setter Property="FontSize" Value="31"/>
        <Setter Property="Foreground" Value="#FF220B00" />
        <Setter Property="FontWeight" Value="Bold" />
        <Setter Property="FontFamily" Value="Century Gothic"/>
    </Style>

    <Style x:Key="MessageHeaderTextStyle" TargetType="TextBlock">
        <Setter Property="FontSize" Value="29"/>
        <Setter Property="Foreground" Value="#FFF4FAFC" />
        <Setter Property="FontWeight" Value="Bold" />
        <Setter Property="FontFamily" Value="Century Gothic"/>
    </Style>

    <Style x:Key="MessageTextStyle" TargetType="TextBlock">
        <Setter Property="FontSize" Value="27"/>
        <Setter Property="Foreground" Value="#FFFFFFFF" />
        <Setter Property="FontFamily" Value="Century Gothic"/>
    </Style>

    <Style x:Key="LabelTextStyle" TargetType="TextBlock">
        <Setter Property="FontSize" Value="23"/>
        <Setter Property="Foreground" Value="#FF787582" />
        <Setter Property="FontFamily" Value="Century Gothic"/>
    </Style>

    <Style x:Key="DisableButtonStyle" TargetType="Button">
        <Setter Property="BorderThickness"  Value="3"/>
        <Setter Property="BorderBrush" Value="#FFF1F1F1"/>
        <Setter Property="Foreground" Value="#FF9E9FAB" />
        <Setter Property="FontFamily" Value="Century Gothic"/>
        <Setter Property="FontWeight" Value="SemiBold" />
        <Setter Property="Padding" Value="62,10,62,10"/>
    </Style>

    <Style x:Key="EnableButtonStyle" TargetType="Button">
        <Setter Property="BorderThickness"  Value="3"/>
        <Setter Property="BorderBrush" Value="#FF67B5EE"/>
        <Setter Property="Foreground" Value="White" />
        <Setter Property="Background" Value="#FF67B5EE" />
        <Setter Property="FontFamily" Value="Century Gothic"/>
        <Setter Property="FontWeight" Value="SemiBold" />
        <Setter Property="Padding" Value="62,10,62,10"/>
    </Style>

    <Style x:Key="CancelButtonStyle" TargetType="Button">
        <Setter Property="BorderThickness"  Value="3"/>
        <Setter Property="BorderBrush" Value="#FFF1F1F1"/>
        <Setter Property="Foreground" Value="#FF67B5EE" />
        <Setter Property="FontFamily" Value="Century Gothic"/>
        <Setter Property="FontWeight" Value="SemiBold" />
        <Setter Property="Padding" Value="62,10,62,10"/>
    </Style>

    <Style x:Key="PrimaryNumberTextBoxStyle" TargetType="TextBox">
        <Setter Property="FontSize" Value="35"/>
        <Setter Property="Foreground" Value="#FF000000"/>
        <Setter Property="FontFamily" Value="Century Gothic"/>
        <Setter Property="TextAlignment" Value="Left"/>
        <Setter Property="Margin" Value="-20 0"/>
    </Style>

    <Style x:Key="SecondaryNumberTextBoxStyle" TargetType="TextBox">
        <Setter Property="FontSize" Value="29"/>
        <Setter Property="Foreground" Value="#FF000000"/>
        <Setter Property="FontFamily" Value="Century Gothic"/>
        <Setter Property="TextAlignment" Value="Left"/>
        <Setter Property="Background" Value="#FFF1F1F1"/>
        <Setter Property="Padding" Value="0"/>
    </Style>      

    <Style x:Key="HyperlinkTextBoxStyle" TargetType="TextBlock">
        <Setter Property="FontSize" Value="20"/>
        <Setter Property="Foreground" Value="#FF79A1CA"/>
    </Style>

</ResourceDictionary>